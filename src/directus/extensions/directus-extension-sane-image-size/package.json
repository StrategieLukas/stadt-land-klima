{
  "name": "directus-extension-sane-image-size",
  "description": "This extension modifies big images after upload to a preset maximum (width, height & quality).",
  "icon": "Image",
  "version": "1.1.11",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook",
    "image"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "AUTHORS",
    "LICENSE"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^13.0.0"
  }
}
